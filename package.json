{
  "name": "my-app",
  "private": true,
  "type": "module",
  "main": "dist/worker/script.js",
  "engines": {
    "node": ">=14.17"
  },
  "scripts": {
    "dev": "vitedge --port 3333 --open --ssr",
    "dev:spa": "vitedge --port 3333 --open",
    "build": "rm -rf dist && vitedge build",
    "preview": "vitedge preview",
    "preview:watch": "vitedge preview --build-watch",
    "preview:wrangler": "wrangler dev",
    "deploy": "wrangler publish"
  },
  "dependencies": {
    "@vueuse/core": "^5.2.0",
    "@vueuse/head": "^0.6.0",
    "nprogress": "^0.2.0",
    "prism-theme-vars": "^0.2.2",
    "vitedge": "0.16.x",
    "vue": "3.2.4",
    "vue-i18n": "^9.1.7",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.7.0",
    "@cloudflare/workers-types": "^2.2.2",
    "@iconify/json": "^1.1.380",
    "@intlify/vite-plugin-vue-i18n": "^2.4.0",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@vitejs/plugin-vue": "1.4.0",
    "@vue/compiler-sfc": "3.2.4",
    "@vue/server-renderer": "3.2.4",
    "cross-env": "^7.0.3",
    "esbuild": "^0.12.7",
    "eslint": "^7.31.0",
    "markdown-it-prism": "^2.1.8",
    "miniflare": "^1.3.3",
    "node-fetch": "^2.6.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "vite": "^2.4.4",
    "vite-plugin-components": "^0.13.2",
    "vite-plugin-icons": "^0.6.5",
    "vite-plugin-md": "^0.9.0",
    "vite-plugin-pages": "^0.15.1",
    "vite-plugin-pwa": "^0.8.2",
    "vite-plugin-vue-layouts": "^0.3.1",
    "vite-plugin-windicss": "^1.2.7"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config",
    "rules": {
      "no-unused-vars": "off",
      "no-console": "off",
      "comma-dangle": "off",
      "brace-style": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}
